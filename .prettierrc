{
    "trailingComma": "es5",
    "tabWidth": 4,
    "endOfLine": "lf",
    "printWidth": 80,
    "useTabs": false,
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "singleQuote": false,
    "jsxBracketSameLine": false,
    "jsxSingleQuote": false,
    "overrides": [
        {
            "files": "*.{scss,sass}",
            "options": {
                "parser": "scss"
            }
        }
    ]
}
